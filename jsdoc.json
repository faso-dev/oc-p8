{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "./js",
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "static": true,
    "sort": true
  }
}